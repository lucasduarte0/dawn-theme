        {% form 'storefront_password', class: 'form space-y-6 md:space-y-5' %}
            {% if form.errors != blank %}
                <div class="font-matter text-13">{{ form.errors | default_errors }}</div>
            {% endif %}
            {% comment %} <label for="password">Enter store using password:</label> {% endcomment %}
            <input type="password" name="password" id="password" autofocus="" class="form__input tracking-widest border border-brown placeholder:text-brown" placeholder="Password" />
            <input class="button" name="commit" type="submit" value="Enter" />
        {% endform %}
